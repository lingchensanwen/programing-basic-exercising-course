   "source": [
    "a = [1,6,8,9,2,7,3,16,27]\n",
    "j = len(a)\n",
    "while j > 0:\n",
    "    i = 0\n",
    "    while i < j-1:\n",
    "        if a[i] > a[i+1]:\n",
    "            temp = a[i+1]\n",
    "            a[i+1] = a[i]\n",
    "            a[i] = temp\n",
    "        i = i + 1\n",
    "    j = j - 1\n",
    "print(a)    "
   ]
  }

    "import math\n",
    "f = lambda x:x + x **2\n",
    "def nintergreate(f, start, end, number):\n",
    "    sum = 0\n",
    "    average = (start - end)/number\n",
    "    for i in range(start, end):\n",
    "        sum = sum + (f(i) + f(i+average))/2\n",
    "    return sum"
   ]
